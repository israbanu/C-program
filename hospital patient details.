#include <stdio.h>

void displayMenu();   // Function declaration

int main()
{
    displayMenu();   // Function call
    return 0;
}

void displayMenu()    // Function definition
{
    printf("----- MENU -----\n");
    printf("1. Add\n");
    printf("2. Subtract\n");
    printf("3. Multiply\n");
    printf("4. Divide\n");
    printf("5. Exit\n");
}